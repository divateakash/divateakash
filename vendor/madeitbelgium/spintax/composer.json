{
    "version": "1.3.0",
    "name": "madeitbelgium/spintax",
    "description": "Generate multiple artikels based on one text.",
    "keywords": ["SEO", "onpage", "content", "bulk", "copywriting", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tjebbe Lievens",
            "email": "tjebbe.lievens@madeit.be"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "~5.0|6.*|7.*|8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*|6.*|8.*|9.*",
        "php-coveralls/php-coveralls": "~2.1"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "MadeITBelgium\\Spintax\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadeITBelgium\\Spintax\\SpintaxServiceProvider"
            ],
            "aliases": {
                "Spintax": "MadeITBelgium\\Spintax\\SpintaxFacade"
            }
        }
    }
}
